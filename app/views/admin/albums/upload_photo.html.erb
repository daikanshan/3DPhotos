<ul class="nav nav-tabs">
  <li role="presentation" class="active" > <%= link_to "从本地上传" ,admin_upload_path+"/"+@admin_album.name %></li>
  <li role="presentation" > <%= link_to "从相片库上传" ,"/admin/upload_online/"+@admin_album.name  %></li>
</ul>
<%= form_for @admin_photo, url:admin_upload_path do |f| %>
  <%= form_errors @admin_photo %>
  <div class="form-group">
    <%= f.label "上传图片" %>
    <%= image_tag(@admin_photo.img_url(:thumb)) if @admin_photo.img? %>
    <%= f.file_field :img %>
    <%= f.hidden_field :img_cache %>
  </div>

  <div class="type">
    <%= f.hidden_field :album_id,value:@admin_album.id %>
  </div>

  <div class="form-group action">
    <%= link_to '取消', admin_photos_path,class:"btn btn-default cancle" %>
    <%= f.submit class:"btn btn-primary submit",id:"submit" %>
  </div>

<% end %>
