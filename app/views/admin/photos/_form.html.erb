<%= form_tag admin_photos_upload_path , multipart: true do %>
  <!-- <iframe name="uploadFrame" src="" style="display: none;"></iframe> -->
  <%= form_errors @admin_photo %>

  <div class="form-group">
    
  </div>
  <div class="form-group">
  <div class="upload-div">
     <input type="file" name="admin_photo_imgs[]" class="uploadInput" multiple="multiple" />
     <div class="uploadShow">
      <span class="glyphicon glyphicon-plus"></span>
      <i>从本地选择要上传的相片</i>
     </div>
  </div>
  </div>
  
  <div class="form-group action">
    <%= link_to '取消', admin_photos_path,class:"btn btn-default cancle" %>
    <%= submit_tag 'upload', class:"btn btn-primary submit waiting",id:"submit" %>
  </div>
<% end %>