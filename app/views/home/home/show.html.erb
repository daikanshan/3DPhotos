<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, user-scalable=no, minimum-scale=1.0, maximum-scale=1.0">
<title>Elements</title>
<%= stylesheet_link_tag  "home/texiao/#{@album.effect.name}" %>
</head>
<body>
<%= render "related/texiao/#{@album.effect.name}" %>
<script>
var table_img_large = [];
var table_img_medium = [];
var table_img_panorama = [];
var table_img = [];
<% max = @photos.count %>
<% for i in 0..max-1 do %>
table_img_large[<%= i %>] = "<%= @photos[i%max].img_url(:medium) %>"
table_img_medium[<%= i %>] = "<%= @photos[i%max].img_url(:thumb) %>"
table_img_panorama[<%= i %>] = "<%= @photos[i%max].img_url(:panorama) %>"
table_img[<%= i %>] = "<%= @photos[i%max].img_url %>"
<% end %>
</script>

<%= javascript_include_tag "home/texiao/#{@album.effect.name}" %>
</body>
</html>
